    "@types/jest": "^29.2.3",
    "@types/node": "^18.19.22",
    "@aws-sdk": "^2.7.0",
    "aws-sdk": "^2.1582.0",
    "dotenv": "^16.0.3",
    "jest": "^29.3.1",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-esbuild": "^1.51.0",
    "serverless-offline": "^13.3.3",
